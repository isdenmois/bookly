{
  "name": "bookly",
  "version": "4.16.0",
  "private": true,
  "scripts": {
    "start": "react-native start",
    "android": "react-native run-android",
    "postinstall": "patch-package",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx src --quiet",
    "lint:fix": "yarn lint --fix",
    "clean": "android/gradlew -p android clean",
    "build": "node dev-tools/release",
    "release": "node dev-tools/release --clean --upload",
    "cucumber": "cross-env NODE_ENV=test cucumber-js e2e/screens --format=progress-bar --exit",
    "e2e": "yarn e2e:build && yarn e2e:run",
    "e2e:run": "yarn cucumber --configuration=android.emu.release",
    "e2e:build": "detox build -c android.emu.release",
    "e2e:debug": "yarn e2e:debug:build && npm run e2e:debug:run",
    "e2e:debug:run": "yarn cucumber --configuration=android.emu.debug",
    "e2e:debug:build": "node dev-tools/release --test --type=debug",
    "e2e:metro": "cross-env NODE_ENV=test RN_SRC_EXT=e2e.ts react-native start --reset-cache",
    "changelog": "node dev-tools/changelog --type=debug",
    "web": "webpack serve --host 0.0.0.0",
    "web:build": "cross-env NODE_ENV=production webpack --progress",
    "web:stats": "cross-env NODE_ENV=production JSON=1 webpack -p --json > dist/stats.json",
    "analyze": "react-native-bundle-visualizer --platform android",
    "create-patch": "patch-package --exclude android/build/",
    "dev-bundle": "react-native bundle --dev true --platform android --entry-file index.js --bundle-output ./dist/bundle.js"
  },
  "dependencies": {
    "@nozbe/watermelondb": "0.22.0",
    "@nozbe/with-observables": "1.3.0",
    "@react-native-community/async-storage": "1.12.1",
    "@react-native-community/datetimepicker": "3.5.0",
    "@react-native-community/masked-view": "0.1.11",
    "@react-navigation/bottom-tabs": "5.11.11",
    "@react-navigation/native": "5.9.4",
    "@react-navigation/stack": "5.14.5",
    "@shopify/restyle": "1.4.0",
    "bbcode-to-react": "0.2.9",
    "i18next": "20.3.1",
    "i18next-react-native-language-detector": "1.0.2",
    "lodash": "4.17.21",
    "pluralize-ru": "1.0.1",
    "quagga": "0.12.1",
    "rambdax": "3.7.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-native": "0.64.1",
    "react-native-camera": "3.43.9",
    "react-native-dynamic": "1.0.0",
    "react-native-fast-image": "8.3.4",
    "react-native-gesture-handler": "1.10.3",
    "react-native-locale-detector": "1.0.1",
    "react-native-modal-datetime-picker": "10.0.0",
    "react-native-navigation-bar-color": "2.0.1",
    "react-native-pager-view": "5.1.10",
    "react-native-reanimated": "2.2.0",
    "react-native-safe-area-context": "3.2.0",
    "react-native-screens": "3.3.0",
    "react-native-splash-screen": "3.2.0",
    "react-native-svg": "12.1.1",
    "react-native-tab-view": "3.0.1",
    "react-native-triangle": "0.0.9",
    "react-native-vector-icons": "8.1.0",
    "react-native-web": "0.16.3",
    "recyclerlistview": "3.0.5",
    "rn-classnames": "1.0.1",
    "rxjs": "6.5.3",
    "setimmediate": "1.0.5",
    "workbox-window": "6.1.5"
  },
  "devDependencies": {
    "@babel/core": "7.13.10",
    "@babel/plugin-proposal-class-properties": "7.13.0",
    "@babel/plugin-proposal-decorators": "7.13.5",
    "@babel/plugin-syntax-flow": "7.12.13",
    "@babel/plugin-transform-flow-strip-types": "7.13.0",
    "@babel/plugin-transform-runtime": "7.13.10",
    "@babel/preset-env": "7.13.10",
    "@babel/preset-react": "7.12.13",
    "@babel/preset-typescript": "7.13.0",
    "@babel/runtime": "7.13.10",
    "@react-native-community/eslint-config": "2.0.0",
    "@types/jest": "26.0.23",
    "@types/lodash": "4.14.170",
    "@types/react": "17.0.8",
    "@types/react-native": "0.64.7",
    "@types/react-native-calendars": "1.505.3",
    "@types/react-native-vector-icons": "6.4.6",
    "@types/react-test-renderer": "17.0.1",
    "@typescript-eslint/eslint-plugin": "4.25.0",
    "@typescript-eslint/parser": "4.25.0",
    "babel-loader": "8.2.2",
    "babel-plugin-jsx-remove-data-test-id": "3.0.0",
    "babel-plugin-lodash": "3.3.4",
    "babel-plugin-module-resolver": "4.1.0",
    "babel-plugin-transform-class-properties": "6.24.1",
    "clean-webpack-plugin": "4.0.0-alpha.0",
    "copy-webpack-plugin": "9.0.0",
    "cross-env": "7.0.3",
    "cucumber": "6.0.5",
    "detox": "17.9.0",
    "esbuild-loader": "^2.13.1",
    "eslint": "7.27.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-config-react": "1.1.7",
    "eslint-plugin-jest": "24.3.6",
    "eslint-plugin-prettier": "3.4.0",
    "file-loader": "6.2.0",
    "hoist-non-react-statics": "3.3.2",
    "html-webpack-plugin": "5.3.1",
    "jest": "27.0.3",
    "metro-minify-terser": "0.66.0",
    "metro-react-native-babel-preset": "0.66.0",
    "mockdate": "3.0.5",
    "patch-package": "6.4.7",
    "postinstall-postinstall": "2.1.0",
    "prettier": "2.3.0",
    "pretty-quick": "3.1.0",
    "process": "0.11.10",
    "react-native-bundle-visualizer": "2.2.1",
    "react-native-dotenv": "2.5.5",
    "react-native-testing-library": "6.0.0",
    "react-native-typescript-transformer": "1.2.13",
    "react-test-renderer": "17.0.2",
    "remove-flow-types-loader": "^1.1.0",
    "simple-git-hooks": "2.4.1",
    "ts-loader": "9.2.2",
    "typescript": "4.3.2",
    "url-loader": "4.1.1",
    "webpack": "5.38.1",
    "webpack-cli": "4.7.0",
    "webpack-dev-server": "3.11.2",
    "why-did-you-update": "1.0.8",
    "workbox-webpack-plugin": "6.1.5",
    "worker-loader": "3.0.8"
  },
  "resolutions": {
    "**/hoist-non-react-statics": "3.3.2",
    "**/fbjs": "1.0.0",
    "**/rxjs": "6.5.3",
    "**/rambdax": "3.7.0",
    "**/isarray": "1.0.0",
    "**/is-arrayish": "0.3.2",
    "**/color-name": "1.1.4",
    "**/lodash": "4.17.21"
  },
  "detox": {
    "configurations": {
      "android.emu.debug": {
        "binaryPath": "android/app/build/outputs/apk/debug/app-debug.apk",
        "build": "node dev-tools/release --test --type=debug",
        "type": "android.attached",
        "device": {
          "adbName": "emulator-5554"
        }
      },
      "android.emu.release": {
        "binaryPath": "android/app/build/outputs/apk/release/app-release.apk",
        "build": "node dev-tools/release --clean --test",
        "type": "android.attached",
        "device": {
          "adbName": "emulator-5554"
        }
      }
    },
    "test-runner": "jest"
  },
  "simple-git-hooks": {
    "pre-commit": "pretty-quick --staged"
  },
  "browserslist": [
    "ios 14"
  ]
}
