    "webpack-cli": "4.9.1",
    "webpack-dev-server": "4.3.1",
    "webpack-virtual-modules": "0.4.3",
    "why-did-you-update": "1.0.8",
    "workbox-webpack-plugin": "5.1.4",
    "worker-loader": "3.0.8"
  },
  "resolutions": {
    "**/hoist-non-react-statics": "3.3.2",
    "**/fbjs": "1.0.0",
    "**/rxjs": "6.5.3",
    "**/rambdax": "3.7.0",
    "**/isarray": "1.0.0",
    "**/is-arrayish": "0.3.2",
    "**/color-name": "1.1.4",
    "**/lodash": "4.17.21"
  },
  "detox": {
    "configurations": {
      "android.emu.debug": {
        "binaryPath": "android/app/build/outputs/apk/debug/app-debug.apk",
        "build": "node dev-tools/release --test --type=debug",
        "type": "android.attached",
        "device": {
          "adbName": "emulator-5554"
        }
      },
      "android.emu.release": {
        "binaryPath": "android/app/build/outputs/apk/release/app-release.apk",
        "build": "node dev-tools/release --clean --test",
        "type": "android.attached",
        "device": {
          "adbName": "emulator-5554"
        }
      }
    },
    "test-runner": "jest"
  },
  "simple-git-hooks": {
    "pre-commit": "pretty-quick --staged"
  },
  "browserslist": [
    "ios 14"
  ]
}
