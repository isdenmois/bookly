    "babel-plugin-module-resolver": "4.0.0",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-preset-mobx": "2.0.0",
    "clean-webpack-plugin": "3.0.0",
    "copy-webpack-plugin": "6.1.0",
    "cucumber": "6.0.5",
    "detox": "17.4.6",
    "eslint": "7.8.1",
    "eslint-config-prettier": "6.11.0",
    "eslint-config-react": "1.1.7",
    "eslint-plugin-jest": "24.0.0",
    "eslint-plugin-prettier": "3.1.4",
    "file-loader": "6.1.0",
    "hoist-non-react-statics": "3.3.2",
    "html-webpack-plugin": "4.4.1",
    "husky": "4.2.5",
    "jest": "26.4.2",
    "jetifier": "1.6.6",
    "metro-minify-terser": "0.63.0",
    "metro-react-native-babel-preset": "0.63.0",
    "mockdate": "3.0.2",
    "patch-package": "6.2.2",
    "postinstall-postinstall": "2.1.0",
    "prettier": "2.1.1",
    "pretty-quick": "3.0.0",
    "proxy-polyfill": "0.3.2",
    "react-native-bundle-visualizer": "2.2.1",
    "react-native-dotenv": "2.4.0",
    "react-native-testing-library": "6.0.0",
    "react-native-typescript-transformer": "1.2.13",
    "react-test-renderer": "16.13.1",
    "terser-webpack-plugin": "4.1.0",
    "ts-loader": "8.0.3",
    "typescript": "4.0.2",
    "url-loader": "4.1.0",
    "webpack": "4.44.1",
    "webpack-cli": "3.3.12",
    "webpack-dev-server": "3.11.0",
    "why-did-you-update": "1.0.8",
    "workbox-webpack-plugin": "5.1.3",
    "worker-loader": "2.0.0"
  },
  "resolutions": {
    "**/hoist-non-react-statics": "3.3.2",
    "**/fbjs": "1.0.0",
    "**/rxjs": "6.6.3",
    "**/rambdax": "3.7.0",
    "**/isarray": "1.0.0",
    "**/is-arrayish": "0.3.2",
    "**/color-name": "1.1.4",
    "**/lodash": "4.17.20"
  },
  "detox": {
    "configurations": {
      "android.emu.debug": {
        "binaryPath": "android/app/build/outputs/apk/debug/app-debug.apk",
        "build": "node dev-tools/release --test --type=debug",
        "type": "android.attached",
        "device": {
          "adbName": "emulator-5554"
        }
      },
      "android.emu.release": {
        "binaryPath": "android/app/build/outputs/apk/release/app-release.apk",
        "build": "node dev-tools/release --clean --test",
        "type": "android.attached",
        "device": {
          "adbName": "emulator-5554"
        }
      }
    },
    "test-runner": "jest"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "browserslist": [
    "ios 13.2"
  ]
}
