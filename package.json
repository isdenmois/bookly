{
  "name": "bookly",
  "version": "4.17.0",
  "private": true,
  "scripts": {
    "start": "react-native start",
    "android": "react-native run-android",
    "postinstall": "patch-package",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx src --quiet",
    "lint:fix": "yarn lint --fix",
    "clean": "android/gradlew -p android clean",
    "build": "NODE_ENV=production node dev-tools/release",
    "release": "NODE_ENV=production node dev-tools/release --clean --upload",
    "cucumber": "cross-env NODE_ENV=test cucumber-js e2e/screens --format=progress-bar --exit",
    "e2e": "yarn e2e:build && yarn e2e:run",
    "e2e:run": "yarn cucumber --configuration=android.emu.release",
    "e2e:build": "NODE_ENV=production detox build -c android.emu.release",
    "e2e:debug": "yarn e2e:debug:build && npm run e2e:debug:run",
    "e2e:debug:run": "yarn cucumber --configuration=android.emu.debug",
    "e2e:debug:build": "node dev-tools/release --test --type=debug",
    "e2e:metro": "cross-env NODE_ENV=test RN_SRC_EXT=e2e.ts react-native start --reset-cache",
    "changelog": "node dev-tools/changelog --type=debug",
    "web": "webpack serve --host 0.0.0.0",
    "web:build": "cross-env NODE_ENV=production webpack --progress",
    "web:stats": "cross-env NODE_ENV=production JSON=1 webpack -p --json > dist/stats.json",
    "analyze": "react-native-bundle-visualizer --platform android",
    "create-patch": "patch-package --exclude android/build/",
    "dev-bundle": "react-native bundle --dev true --platform android --entry-file index.js --bundle-output ./dist/bundle.js"
  },
  "dependencies": {
    "@nozbe/watermelondb": "0.22.0",
    "@nozbe/with-observables": "1.4.0",
    "@react-native-async-storage/async-storage": "1.15.9",
    "@react-native-community/datetimepicker": "3.5.2",
    "@react-native-community/masked-view": "0.1.11",
    "@react-navigation/bottom-tabs": "6.0.9",
    "@react-navigation/native": "6.0.6",
    "@react-navigation/stack": "6.0.11",
    "@shopify/restyle": "1.4.0",
    "bbcode-to-react": "0.2.9",
    "i18next": "20.3.1",
    "i18next-react-native-language-detector": "1.0.2",
    "lodash": "4.17.21",
    "pluralize-ru": "1.0.1",
    "quagga": "0.12.1",
    "rambdax": "3.7.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-native": "0.66.1",
    "react-native-camera": "4.2.1",
    "react-native-dynamic": "1.0.0",
    "react-native-fast-image": "8.3.4",
    "react-native-gesture-handler": "1.10.3",
    "react-native-locale-detector": "1.0.1",
    "react-native-modal-datetime-picker": "13.0.0",
    "react-native-navigation-bar-color": "2.0.1",
    "react-native-pager-view": "5.4.7",
    "react-native-reanimated": "2.3.0-beta.3",
    "react-native-safe-area-context": "3.3.2",
    "react-native-screens": "3.6.0",
    "react-native-splash-screen": "3.2.0",
    "react-native-svg": "12.1.1",
    "react-native-tab-view": "3.1.1",
    "react-native-triangle": "0.0.9",
    "react-native-vector-icons": "9.0.0",
    "react-native-web": "0.17.5",
    "recyclerlistview": "3.0.5",
    "rn-classnames": "1.0.1",
    "rxjs": "6.5.3",
    "setimmediate": "1.0.5",
    "workbox-window": "6.1.2"
