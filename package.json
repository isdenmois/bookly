{
  "name": "bookly",
  "version": "3.21.1",
  "private": true,
  "scripts": {
    "start": "react-native start",
    "postinstall": "patch-package",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx src",
    "lint:fix": "yarn lint --fix",
    "clean": "android/gradlew.bat -p android clean",
    "build": "node dev-tools/release",
    "prerelease": "node dev-tools/changelog",
    "release": "node dev-tools/release --clean --upload",
    "e2e": "npm run e2e:build && npm run e2e:run",
    "e2e:run": "detox test -c android.emu.release",
    "e2e:build": "detox build -c android.emu.release",
    "e2e:debug": "npm run e2e:debug:build && npm run e2e:debug:run",
    "e2e:debug:run": "detox test -c android.emu.debug",
    "e2e:debug:build": "node dev-tools/release --test --type=debug",
    "e2e:metro": "cross-env NODE_ENV=test RN_SRC_EXT=e2e.ts react-native start --reset-cache",
    "run-android": "react-native run-android",
    "changelog": "node dev-tools/changelog --type=debug"
  },
  "dependencies": {
    "@nozbe/watermelondb": "0.14.1",
    "@nozbe/with-observables": "1.0.5",
    "@react-native-community/async-storage": "1.6.2",
    "bbcode-to-react": "0.2.9",
    "lodash": "4.17.15",
    "mobx": "5.14.2",
    "mobx-react": "6.1.4",
    "pluralize-ru": "1.0.1",
    "rambdax": "2.14.1",
    "react": "16.9.0",
    "react-native": "0.61.2",
    "react-native-calendars": "1.212.0",
    "react-native-camera": "3.8.0",
    "react-native-fast-image": "7.0.2",
    "react-native-gesture-handler": "1.4.1",
    "react-native-modal-datetime-picker": "7.6.0",
    "react-native-reanimated": "1.3.0",
    "react-native-screens": "1.0.0-alpha.23",
    "react-native-splash-screen": "3.2.0",
    "react-native-tab-view": "2.10.0",
    "react-native-vector-icons": "6.6.0",
    "react-navigation": "4.0.10",
    "react-navigation-props-mapper": "1.0.1",
    "react-navigation-stack": "1.10.3",
    "rn-classnames": "1.0.1",
    "rxjs": "6.5.3"
  },
  "devDependencies": {
    "@babel/core": "7.6.4",
    "@babel/plugin-proposal-class-properties": "7.5.5",
    "@babel/plugin-proposal-decorators": "7.6.0",
    "@babel/plugin-transform-runtime": "7.6.2",
    "@babel/preset-env": "7.6.3",
    "@babel/preset-react": "7.6.3",
    "@babel/runtime": "7.6.3",
    "@react-native-community/eslint-config": "0.0.5",
    "@types/lodash": "4.14.144",
    "@types/react": "16.9.11",
    "@types/react-native": "0.60.21",
    "@types/react-native-calendars": "1.20.7",
    "@types/react-native-vector-icons": "6.4.4",
    "@typescript-eslint/eslint-plugin": "2.5.0",
    "@typescript-eslint/parser": "2.5.0",
    "babel-plugin-jsx-remove-data-test-id": "2.0.1",
    "babel-plugin-module-resolver": "3.2.0",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-preset-mobx": "2.0.0",
    "body-parser": "1.19.0",
    "detox": "14.0.2",
    "eslint": "6.5.1",
    "eslint-config-prettier": "6.4.0",
    "eslint-config-react": "1.1.7",
    "eslint-plugin-prettier": "3.1.1",
    "express": "4.17.1",
    "hermes-engine": "0.1.1",
    "husky": "3.0.9",
    "jest": "24.9.0",
    "jetifier": "1.6.4",
    "metro-minify-terser": "0.56.3",
    "metro-react-native-babel-preset": "0.56.3",
    "patch-package": "6.2.0",
    "postinstall-postinstall": "2.0.0",
    "prettier": "1.18.2",
    "pretty-quick": "2.0.0",
    "proxy-polyfill": "0.3.0",
    "react-native-dotenv": "0.2.0",
    "react-native-typescript-transformer": "1.2.13",
    "reactotron-react-native": "4.0.2",
    "typescript": "3.6.4",
    "why-did-you-update": "1.0.6"
  },
  "resolutions": {
    "**/hoist-non-react-statics": "3.3.0",
    "**/react-native-tab-view": "2.10.0",
    "**/fbjs": "1.0.0"
  },
  "detox": {
    "configurations": {
      "android.emu.debug": {
        "binaryPath": "android/app/build/outputs/apk/debug/app-debug.apk",
        "build": "node dev-tools/release --test --type=debug",
        "type": "android.attached"
      },
      "android.emu.release": {
        "binaryPath": "android/app/build/outputs/apk/release/app-release.apk",
        "build": "node dev-tools/release --clean --test",
        "type": "android.attached"
      }
    },
    "test-runner": "jest"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}
