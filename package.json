{
  "name": "bookly",
  "version": "4.5.0",
  "private": true,
  "scripts": {
    "start": "react-native start",
    "postinstall": "patch-package",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx src --quiet",
    "lint:fix": "yarn lint --fix",
    "clean": "android/gradlew -p android clean",
    "build": "node dev-tools/release",
    "release": "node dev-tools/release --clean --upload",
    "cucumber": "cross-env NODE_ENV=test cucumber-js e2e/screens --format=progress-bar --exit",
    "e2e": "yarn e2e:build && yarn e2e:run",
    "e2e:run": "yarn cucumber --configuration=android.emu.release",
    "e2e:build": "detox build -c android.emu.release",
    "e2e:debug": "yarn e2e:debug:build && npm run e2e:debug:run",
    "e2e:debug:run": "yarn cucumber --configuration=android.emu.debug",
    "e2e:debug:build": "node dev-tools/release --test --type=debug",
    "e2e:metro": "cross-env NODE_ENV=test RN_SRC_EXT=e2e.ts react-native start --reset-cache",
    "run-android": "react-native run-android",
    "changelog": "node dev-tools/changelog --type=debug",
    "web": "webpack-dev-server --host 0.0.0.0",
    "web:build": "cross-env NODE_ENV=production webpack -p --optimize-minimize",
    "web:stats": "cross-env NODE_ENV=production JSON=1 webpack -p --json > dist/stats.json",
    "analyze": "react-native-bundle-visualizer --platform android",
    "create-patch": "patch-package --exclude android/build",
    "dev-bundle": "react-native bundle --dev true --platform android --entry-file index.js --bundle-output ./dist/bundle.js"
  },
  "dependencies": {
    "@nozbe/watermelondb": "0.19.0",
    "@nozbe/with-observables": "1.0.5",
    "@react-native-community/async-storage": "1.12.0",
    "@react-native-community/datetimepicker": "3.0.2",
    "@react-native-community/masked-view": "0.1.10",
    "@react-navigation/web": "1.0.0-alpha.9",
    "bbcode-to-react": "0.2.9",
    "i18next": "19.7.0",
    "i18next-react-native-language-detector": "1.0.2",
    "lodash": "4.17.20",
    "mobx": "5.15.6",
    "mobx-react": "6.3.0",
    "pluralize-ru": "1.0.1",
    "quagga": "0.12.1",
    "rambdax": "3.7.0",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-native": "0.63.2",
    "react-native-calendars": "1.403.0",
    "react-native-camera": "3.38.0",
    "react-native-dynamic": "1.0.0",
    "react-native-fast-image": "8.3.2",
    "react-native-gesture-handler": "1.8.0",
    "react-native-locale-detector": "1.0.1",
    "react-native-modal-datetime-picker": "8.9.3",
    "react-native-navigation-bar-color": "2.0.1",
    "react-native-reanimated": "1.13.0",
    "react-native-safe-area-context": "3.1.7",
    "react-native-screens": "2.10.1",
    "react-native-splash-screen": "3.2.0",
    "react-native-tab-view": "2.15.1",
    "react-native-triangle": "0.0.9",
    "react-native-vector-icons": "7.0.0",
    "react-native-web": "0.13.9",
    "react-navigation": "4.4.0",
    "react-navigation-props-mapper": "1.0.4",
    "react-navigation-stack": "2.8.2",
    "rn-classnames": "1.0.1",
    "rxjs": "6.6.3",
    "workbox-window": "5.1.3"
  },
  "devDependencies": {
    "@babel/core": "7.11.6",
    "@babel/plugin-proposal-class-properties": "7.10.4",
    "@babel/plugin-proposal-decorators": "7.10.5",
    "@babel/plugin-syntax-flow": "7.10.4",
    "@babel/plugin-transform-flow-strip-types": "7.10.4",
    "@babel/plugin-transform-runtime": "7.11.5",
    "@babel/preset-env": "7.11.5",
    "@babel/preset-react": "7.10.4",
    "@babel/preset-typescript": "7.10.4",
    "@babel/runtime": "7.11.2",
    "@react-native-community/eslint-config": "2.0.0",
    "@types/jest": "26.0.13",
    "@types/lodash": "4.14.161",
    "@types/react": "16.9.49",
    "@types/react-native": "0.63.15",
    "@types/react-native-calendars": "1.20.10",
    "@types/react-native-vector-icons": "6.4.5",
    "@types/react-test-renderer": "16.9.3",
    "@typescript-eslint/eslint-plugin": "4.0.1",
    "@typescript-eslint/parser": "4.0.1",
    "babel-loader": "8.1.0",
    "babel-plugin-jsx-remove-data-test-id": "2.1.3",
