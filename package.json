{
  "name": "bookly",
  "version": "3.8.0",
  "private": true,
  "scripts": {
    "start": "react-native start",
    "postinstall": "patch-package",
    "lint": "tslint --project tsconfig.json",
    "clean": "android/gradlew.bat -p android clean",
    "build": "node dev-tools/release",
    "release": "node dev-tools/release --clean --upload",
    "e2e": "npm run e2e:build && npm run e2e:run",
    "e2e:run": "detox test -c android.emu.release",
    "e2e:build": "detox build -c android.emu.release",
    "e2e:debug": "npm run e2e:debug:build && npm run e2e:debug:run",
    "e2e:debug:run": "detox test -c android.emu.debug",
    "e2e:debug:build": "node dev-tools/release --test --type=debug",
    "e2e:metro": "cross-env NODE_ENV=test RN_SRC_EXT=e2e.ts react-native start --reset-cache",
    "run-android": "react-native run-android"
  },
  "dependencies": {
    "@nozbe/watermelondb": "0.14.0",
    "@nozbe/with-observables": "1.0.3",
    "@react-native-community/async-storage": "1.6.1",
    "bbcode-to-react": "0.2.9",
    "date-fns": "1.30.1",
    "lodash": "4.17.15",
    "mobx": "5.13.0",
    "mobx-react": "6.1.1",
    "rambdax": "2.12.3",
    "react": "16.8.6",
    "react-native": "0.60.4",
    "react-native-calendars": "1.204.0",
    "react-native-fast-image": "7.0.2",
    "react-native-gesture-handler": "1.3.0",
    "react-native-modal-datetime-picker": "7.5.0",
    "react-native-reanimated": "1.1.0",
    "react-native-screens": "1.0.0-alpha.23",
    "react-native-splash-screen": "3.2.0",
    "react-native-tab-view": "2.9.0",
    "react-native-vector-icons": "6.6.0",
    "react-navigation": "3.11.1",
    "react-navigation-props-mapper": "1.0.1",
    "react-navigation-stack": "https://github.com/isdenmois/stack/releases/download/v1.4.1/react-navigation-stack-1.4.1.tgz",
    "rn-classnames": "1.0.1",
    "rxjs": "6.5.2"
  },
  "devDependencies": {
    "@babel/core": "7.5.5",
    "@babel/plugin-proposal-class-properties": "7.5.5",
    "@babel/plugin-proposal-decorators": "7.4.4",
    "@babel/plugin-transform-runtime": "7.5.5",
    "@babel/preset-env": "7.5.5",
    "@babel/preset-react": "7.0.0",
    "@react-native-community/eslint-config": "0.0.5",
    "@types/lodash": "4.14.136",
    "@types/react": "16.8.24",
    "@types/react-native": "0.60.3",
    "@types/react-native-calendars": "1.20.7",
    "@types/react-native-vector-icons": "6.4.1",
    "babel-eslint": "10.0.2",
    "babel-plugin-jsx-remove-data-test-id": "2.0.0",
    "babel-plugin-module-resolver": "3.2.0",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-preset-mobx": "2.0.0",
    "body-parser": "1.19.0",
    "detox": "13.3.2",
    "eslint": "6.0.1",
    "eslint-config-airbnb": "17.1.1",
    "eslint-config-prettier": "6.0.0",
    "eslint-plugin-import": "2.18.0",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-prettier": "3.1.0",
    "eslint-plugin-react": "7.14.2",
    "express": "4.17.1",
    "jest": "24.8.0",
    "jetifier": "1.6.3",
    "metro-react-native-babel-preset": "0.55.0",
    "patch-package": "6.1.2",
    "postinstall-postinstall": "2.0.0",
    "prettier": "1.18.2",
    "proxy-polyfill": "0.3.0",
    "react-native-dotenv": "0.2.0",
    "react-native-typescript-transformer": "1.2.13",
    "reactotron-react-native": "3.6.4",
    "tslint": "5.18.0",
    "tslint-config-prettier": "1.18.0",
    "tslint-eslint-rules": "5.4.0",
    "tslint-react": "4.0.0",
    "typescript": "3.5.3"
  },
  "resolutions": {
    "**/hoist-non-react-statics": "3.3.0",
    "**/react-native-tab-view": "2.9.0",
    "**/fbjs": "1.0.0"
  },
  "detox": {
    "configurations": {
      "android.emu.debug": {
        "binaryPath": "android/app/build/outputs/apk/debug/app-debug.apk",
        "build": "node dev-tools/release --test --type=debug",
        "type": "android.attached"
      },
      "android.emu.release": {
        "binaryPath": "android/app/build/outputs/apk/release/app-release.apk",
        "build": "node dev-tools/release --clean --test",
        "type": "android.attached"
      }
    },
    "test-runner": "jest"
  }
}
