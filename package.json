    "patch-package": "6.4.7",
    "postinstall-postinstall": "2.1.0",
    "prettier": "2.4.1",
    "pretty-quick": "3.1.1",
    "process": "0.11.10",
    "react-native-bundle-visualizer": "3.0.0",
    "react-native-dotenv": "3.2.0",
    "react-native-testing-library": "6.0.0",
    "react-native-typescript-transformer": "1.2.13",
    "react-test-renderer": "17.0.2",
