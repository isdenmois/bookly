{
  "name": "bookly",
  "version": "3.26.0",
  "private": true,
  "scripts": {
    "start": "react-native start",
    "postinstall": "patch-package",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx src --quiet",
    "lint:fix": "yarn lint --fix",
    "clean": "android/gradlew.bat -p android clean",
    "build": "node dev-tools/release",
    "prerelease": "node dev-tools/changelog",
    "release": "node dev-tools/release --clean --upload",
    "cucumber": "cross-env NODE_ENV=test cucumber-js e2e/screens --format=progress-bar --exit",
    "e2e": "yarn e2e:build && yarn e2e:run",
    "e2e:run": "yarn cucumber --configuration=android.emu.release",
    "e2e:build": "detox build -c android.emu.release",
    "e2e:debug": "yarn e2e:debug:build && npm run e2e:debug:run",
    "e2e:debug:run": "yarn cucumber --configuration=android.emu.debug",
    "e2e:debug:build": "node dev-tools/release --test --type=debug",
    "e2e:metro": "cross-env NODE_ENV=test RN_SRC_EXT=e2e.ts react-native start --reset-cache",
    "run-android": "react-native run-android",
    "changelog": "node dev-tools/changelog --type=debug"
  },
  "dependencies": {
    "@nozbe/watermelondb": "0.14.1",
    "@nozbe/with-observables": "1.0.5",
    "@react-native-community/async-storage": "1.7.1",
    "@react-native-community/masked-view": "0.1.5",
    "bbcode-to-react": "0.2.9",
    "lodash": "4.17.15",
    "mobx": "5.15.1",
    "mobx-react": "6.1.4",
    "pluralize-ru": "1.0.1",
    "rambdax": "2.14.1",
    "react": "16.9.0",
    "react-native": "0.61.5",
    "react-native-calendars": "1.220.0",
    "react-native-camera": "3.15.1",
    "react-native-fast-image": "7.0.2",
    "react-native-gesture-handler": "1.5.3",
    "react-native-modal-datetime-picker": "7.6.1",
    "react-native-reanimated": "1.4.0",
    "react-native-safe-area-context": "0.6.2",
    "react-native-screens": "1.0.0-alpha.23",
    "react-native-splash-screen": "3.2.0",
    "react-native-tab-view": "2.11.0",
    "react-native-vector-icons": "6.6.0",
    "react-navigation": "4.0.10",
    "react-navigation-props-mapper": "1.0.2",
    "react-navigation-stack": "2.0.5",
    "rn-classnames": "1.0.1",
    "rxjs": "6.5.3"
  },
  "devDependencies": {
    "@babel/core": "7.7.7",
    "@babel/plugin-proposal-class-properties": "7.7.4",
    "@babel/plugin-proposal-decorators": "7.7.4",
    "@babel/plugin-transform-runtime": "7.7.6",
    "@babel/preset-env": "7.7.7",
    "@babel/preset-react": "7.7.4",
    "@babel/runtime": "7.7.7",
    "@react-native-community/eslint-config": "0.0.6",
    "@types/lodash": "4.14.149",
    "@types/react": "16.9.17",
    "@types/react-native": "0.60.27",
    "@types/react-native-calendars": "1.20.7",
    "@types/react-native-vector-icons": "6.4.5",
    "@typescript-eslint/eslint-plugin": "2.14.0",
    "@typescript-eslint/parser": "2.14.0",
    "babel-plugin-jsx-remove-data-test-id": "2.1.3",
    "babel-plugin-module-resolver": "4.0.0",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-preset-mobx": "2.0.0",
    "cucumber": "6.0.5",
    "detox": "15.0.0",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.9.0",
    "eslint-config-react": "1.1.7",
    "eslint-plugin-prettier": "3.1.2",
    "hermes-engine": "0.2.1",
    "hoist-non-react-statics": "3.3.1",
    "husky": "3.1.0",
    "jest": "24.8.0",
    "jetifier": "1.6.5",
    "metro-minify-terser": "0.56.3",
    "metro-react-native-babel-preset": "0.57.0",
    "patch-package": "6.2.0",
    "postinstall-postinstall": "2.0.0",
    "prettier": "1.19.1",
    "pretty-quick": "2.0.1",
    "proxy-polyfill": "0.3.0",
    "react-native-dotenv": "0.2.0",
    "react-native-typescript-transformer": "1.2.13",
    "reactotron-react-native": "4.0.2",
    "typescript": "3.7.4",
    "why-did-you-update": "1.0.6"
  },
  "resolutions": {
    "**/hoist-non-react-statics": "3.3.1",
    "**/react-native-tab-view": "2.11.0",
    "**/fbjs": "1.0.0"
  },
  "detox": {
    "configurations": {
      "android.emu.debug": {
        "binaryPath": "android/app/build/outputs/apk/debug/app-debug.apk",
        "build": "node dev-tools/release --test --type=debug",
        "type": "android.attached",
        "device": {
          "adbName": "emulator-5554"
        }
      },
      "android.emu.release": {
        "binaryPath": "android/app/build/outputs/apk/release/app-release.apk",
        "build": "node dev-tools/release --clean --test",
        "type": "android.attached",
        "device": {
          "adbName": "emulator-5554"
        }
      }
    },
    "test-runner": "jest"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}
